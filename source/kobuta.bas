640 HX(C)=IX(R):HY(C)=IY(R):HV(C)=IV(R):HP(C)=0:HF(C)=0:HT(C)=II 'INT(RND(1)*2)
650 P=(HT(C)*8)+(HV(C)*4)+(HP(C)*2)
660 PUT SPRITE C*2  ,(HX(C),HY(C)), 9-HT(C)*4,P
670 PUT SPRITE C*2+1,(HX(C),HY(C)),11+HT(C)*4,P+1
680 RETURN

700 ' ***** CHARACTER MOVE *****
710 IF HY(C)=223 THEN 760
720 IF DF=0 AND HX(C)=CX(HC(C)) AND HF(C)=0 THEN 1000
730 IF HV(C) THEN HX(C)=HX(C)-8 ELSE HX(C)=HX(C)+8
740 HP(C)=HP(C) XOR 1
750 IF (HV(C)=0 AND HX(C)>96+DF*144) OR (HV(C) AND HX(C)<152-DF*152) THEN 800
760 IF DF=0 AND HX(C)=CX(HC(C)) AND W=HC(C) THEN BEEP:HF(C)=1
770 S0=&H1B00+(C*4*2):S1=S0+4:P=(HT(C)*8)+(HV(C)*4)+(HP(C)*2):P0=P*4:P1=(P+1)*4
780 VPOKE S0,HY(C):VPOKE S0+1,HX(C):VPOKE S0+2,P0
790 VPOKE S1,HY(C):VPOKE S1+1,HX(C):VPOKE S1+2,P1:RETURN

800 ' ----- CHARACTER INTO HOUSE -----
810 CC=CC-1:HX(C)=0:HY(C)=223:C(HC(C))=C(HC(C))-1
820 IF DF THEN RETURN 
830 IF HT(C) THEN GM=3 ELSE GOSUB 1900
840 RETURN

1000 ' ----- CHARACTER FALL -----
1010 FOR I=0 TO 31 STEP 2
1020 PUT SPRITE C*2  ,(HX(C),HY(C)+I), 9-HT(C)*4,16+HT(C)*2
1030 PUT SPRITE C*2+1,(HX(C),HY(C)+I),11+HT(C)*4,17+HT(C)*2
1040 SOUND 0,50+I*5:SOUND 1,0:SOUND 8,5+(I\4):SOUND 2,49+I*5:SOUND 3,0:SOUND 9,4+(I\4)
1050 NEXT I:SOUND 8,0:SOUND 9,0
